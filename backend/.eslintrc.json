{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "padded-blocks": "off",
        "consistent-return": "off",
        "max-len":["warn", {"code": 120, "ignoreUrls": true}],
        "no-param-reassign": "off",
        "no-undef": "off",
        "no-return-await": "off",
        "class-methods-use-this": "off",
        "object-curly-spacing": ["warn", "always"],
        "eqeqeq": "off",
        "no-unused-vars": "warn",
        "no-empty-function": "warn"
    }
}
